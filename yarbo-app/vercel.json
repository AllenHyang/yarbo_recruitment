{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev --port 3002",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "src/pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_VERSION": "18"
  },
  "regions": ["hkg1"],
  "github": {
    "autoJobCancelation": true
  }
}